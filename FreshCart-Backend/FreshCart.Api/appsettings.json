{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SqlConnection": "${SQL_SERVER_CONNECTION_STRING}",  
    "AzureStorage": "${AZURE_STORAGE_ACCOUNT_KEY}"
  },
  "Jwt": {
    "Key": "${JWT_SECRET_KEY}",   
    "Issuer": "http://localhost:5000",
    "Audience": "http://localhost:5000"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  }
}
